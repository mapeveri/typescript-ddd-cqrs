    "http-status": "^1.5.0",
    "jsonwebtoken": "^9.0.0",
    "method-override": "^3.0.0",
    "mongodb": "^5.3.0",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "tslog": "^3.2.0",
    "typeorm": "^0.3.15",
    "uuid": "^9.0.0"
  }
