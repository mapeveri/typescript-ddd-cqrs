{
  "name": "typescript-ddd-cqrs",
  "version": "1.0.0",
  "private": true,
  "description": "typescript-ddd-cqrs backend",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "format-code-check": "prettier --check ./src",
    "format-code": "prettier --write ./src",
    "linter": "eslint 'src/**/*.ts'",
    "linter-fix": "eslint --fix 'src/**/*.ts'",
    "typeorm": "ts-node --require tsconfig-paths/register -r dotenv/config -r ts-node/register/transpile-only ./node_modules/typeorm/cli.js -d src/shared/infrastructure/persistence/typeOrm/dataSource.ts ",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/faker": "^6.6.9",
    "@types/glob": "^7.1.3",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/method-override": "0.0.31",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.16.3",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "faker": "^5.5.3",
    "jest": "^29.5.0",
    "nodemon": "^2.0.13",
    "prettier": "2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "axios": "^0.24.0",
